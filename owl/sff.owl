     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:dcterms="http://purl.org/dc/terms/"
     xmlns:ic="http://ontology.eil.utoronto.ca/tove/icontact#"
     xmlns:act="http://ontology.eil.utoronto.ca/tove/activity#"
     xmlns:dqv="http://www.w3.org/ns/dqv#"
     xmlns:geo="http://www.geonames.org/ontology/ontology_v3.1.rdf#"
     xmlns:i72="http://ontology.eil.utoronto.ca/ISO21972/iso21972#"
     xmlns:oep="http://www.w3.org/2001/sw/BestPractices/OEP/SimplePartWhole/part.owl#"
     xmlns:org="http://ontology.eil.utoronto.ca/tove/organization#"
