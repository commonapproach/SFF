    <owl:Class rdf:about="#FundingState">
      <rdfs:subClassOf rdf:resource="https://ontology.commonapproach.org/cids#CODE"/>
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
      <rdfs:label xml:lang="en">Funding State</rdfs:label>
      <cids:hasName xml:lang="en">Funding State</cids:hasName>
      <cids:hasDescription xml:lang="en">A value that reflect funding and approval process. The list items should distinguish concepts of ‘applied’, ‘rejected’, and ‘funded’.</cids:hasDescription>
      <rdfs:subClassOf rdf:resource="#forceHasDescription"/>
      <void:inDataset rdf:resource="#datasetdefinition"/>
      <rdfs:isDefinedBy rdf:resource="https://ontology.commonapproach.org/sff-1.0"/>
    </owl:Class>
    
    <sff:FundingState rdf:about="#applied">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:inScheme rdf:resource="#FundingState"/>
      <rdfs:label xml:lang="en">Applied</rdfs:label>
      <cids:hasName xml:lang="en">Applied</cids:hasName>
      <cids:hasDescription xml:lang="en">Funding proposal has been submitted.</cids:hasDescription>
      <rdfs:label xml:lang="fr">Appliqué</rdfs:label>
      <cids:hasName xml:lang="fr">Appliqué</cids:hasName>
      <cids:hasDescription xml:lang="fr">La proposition de financement a été soumise.</cids:hasDescription>
      <void:inDataset rdf:resource="#datasetdefinition"/>
      <rdfs:isDefinedBy rdf:resource="https://ontology.commonapproach.org/sff-1.0"/>
    </sff:FundingState>

    <sff:FundingState rdf:about="#rejected">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:inScheme rdf:resource="#FundingState"/>
      <rdfs:label xml:lang="en">Rejected</rdfs:label>
      <cids:hasName xml:lang="en">Rejected</cids:hasName>
      <cids:hasDescription xml:lang="en">The funding proposal has been rejected.</cids:hasDescription>
      <rdfs:label xml:lang="fr">Rejetée</rdfs:label>
      <cids:hasName xml:lang="fr">Rejetée</cids:hasName>
      <cids:hasDescription xml:lang="fr">La proposition de financement a été rejetée.</cids:hasDescription>
      <void:inDataset rdf:resource="#datasetdefinition"/>
      <rdfs:isDefinedBy rdf:resource="https://ontology.commonapproach.org/sff-1.0"/>
    </sff:FundingState>

    <sff:FundingState rdf:about="#funded">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:inScheme rdf:resource="#FundingState"/>
      <rdfs:label xml:lang="en">Funded</rdfs:label>
      <cids:hasName xml:lang="en">Funded</cids:hasName>
      <cids:hasDescription xml:lang="en">The funding proposal has been accepted and funding has been allocated.</cids:hasDescription>
      <rdfs:label xml:lang="fr">Alloué</rdfs:label>
      <cids:hasName xml:lang="fr">Alloué</cids:hasName>
      <cids:hasDescription xml:lang="fr">La proposition de financement a été acceptée et le financement a été alloué.</cids:hasDescription>
      <void:inDataset rdf:resource="#datasetdefinition"/>
      <rdfs:isDefinedBy rdf:resource="https://ontology.commonapproach.org/sff-1.0"/>
    </sff:FundingState>

    <sff:FundingState rdf:about="#inReview">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:inScheme rdf:resource="#FundingState"/>
      <rdfs:label xml:lang="en">In Review</rdfs:label>
      <cids:hasName xml:lang="en">In Review</cids:hasName>
      <cids:hasDescription xml:lang="en">The funding proposal is being processed by the evaluating organization and is under review.</cids:hasDescription>
      <rdfs:label xml:lang="fr">Sous-évaluation</rdfs:label>
      <cids:hasName xml:lang="fr">Sous-évaluation</cids:hasName>
      <cids:hasDescription xml:lang="fr">La proposition de financement est en cours de traitement par l’organisme évaluateur et est en cours d’examen.</cids:hasDescription>
      <void:inDataset rdf:resource="#datasetdefinition"/>
      <rdfs:isDefinedBy rdf:resource="https://ontology.commonapproach.org/sff-1.0"/>
    </sff:FundingState>

    

    
    <owl:Class rdf:about="#EDGProfile">
      <rdfs:label xml:lang="en">EDG Profile</rdfs:label>
      <cids:hasName xml:lang="en">EDG Profile</cids:hasName>
      <cids:hasDescription xml:lang="en">Information about the number of members belonging to a specific equity deserving group, as part of a board or management team, for a designated time interval.</cids:hasDescription>
      <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#reportedDate"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
            </owl:Restriction>
      </rdfs:subClassOf>
      
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasSize"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#forEDG"/>
                <owl:allValuesFrom rdf:resource="EquityDeservingGroup"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    
    <owl:Class rdf:about="#EquityDeservingGroup">
      <rdfs:label xml:lang="en">Equity Deserving Group</rdfs:label>
      <cids:hasName xml:lang="en">Equity Deserving Group</cids:hasName>
      <cids:hasDescription xml:lang="en">Defines each of the equity deserving groups relevant to the management and board members of an organization.</cids:hasDescription>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasCharacteristic"/>
                <owl:onDataRange rdf:resource="#Characteristic"/>
            </owl:Restriction>
        </rdfs:subClassOf>      
        <rdfs:subClassOf rdf:resource="#forceHasDescription"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#isDefined"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
            </owl:Restriction>
        </rdfs:subClassOf>        
        <void:inDataset rdf:resource="#datasetdefinition"/>
        <rdfs:isDefinedBy rdf:resource="https://ontology.commonapproach.org/sff-1.0"/>        
    </owl:Class>
    
    <owl:DatatypeProperty rdf:about="#hasTeamSize">
      <rdfs:label xml:lang="en">Team Size</rdfs:label>
